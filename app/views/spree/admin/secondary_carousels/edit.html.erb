<%= render partial: 'spree/admin/shared/secondary_carousel_tabs', locals: {current: :details} %>
<%= render :partial => 'spree/admin/shared/error_messages', :locals => {:target => @secondary_carousel} %>

<%= form_for [:admin, @secondary_carousel] do |f| %>
    <fieldset>
      <%= render :partial => 'form', :locals => {:f => f} %>
      <%= render :partial => 'spree/admin/shared/edit_resource_links' %>
    </fieldset>
<% end %>
